logger:
    file: /var/log/open5gs/{{ bsf.log_filename | default ('bsf.log') }}
sbi:
    server:
      no_tls: true
      cacert: /etc/open5gs/tls/ca.crt
      key: /etc/open5gs/tls/bsf.key
      cert: /etc/open5gs/tls/bsf.crt
    client:
      no_tls: true
      cacert: /etc/open5gs/tls/ca.crt
      key: /etc/open5gs/tls/bsf.key
      cert: /etc/open5gs/tls/bsf.crt
bsf:
    sbi:
      - addr: 127.0.0.15
        port: 7777
scp:
    sbi:
      - addr: 127.0.1.10
        port: 7777
parameter:
max:
time:
