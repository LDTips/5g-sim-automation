db_uri: mongodb://localhost/open5gs
logger:
    file: /var/log/open5gs/{{ scp.log_filename | default ('scp.log')}}
sbi:
    server:
      no_tls: true
      cacert: /etc/open5gs/tls/ca.crt
      key: /etc/open5gs/tls/scp.key
      cert: /etc/open5gs/tls/scp.crt
    client:
      no_tls: true
      cacert: /etc/open5gs/tls/ca.crt
      key: /etc/open5gs/tls/scp.key
      cert: /etc/open5gs/tls/scp.crt
scp:
    sbi:
      - addr: 127.0.1.10
        port: 7777
nrf:
    sbi:
      - addr:
          - 127.0.0.10
        port: 7777
parameter:
max:
time:
