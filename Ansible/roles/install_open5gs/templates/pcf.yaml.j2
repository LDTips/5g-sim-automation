db_uri: mongodb://localhost/open5gs
logger:
    file: /var/log/open5gs/{{ pcf.log_filename | default ('pcf.log') }}
sbi:
    server:
      no_tls: true
      cacert: /etc/open5gs/tls/ca.crt
      key: /etc/open5gs/tls/pcf.key
      cert: /etc/open5gs/tls/pcf.crt
    client:
      no_tls: true
      cacert: /etc/open5gs/tls/ca.crt
      key: /etc/open5gs/tls/pcf.key
      cert: /etc/open5gs/tls/pcf.crt
pcf:
    sbi:
      - addr: 127.0.0.13
        port: 7777
    metrics:
      - addr: 127.0.0.13
        port: 9090
scp:
    sbi:
      - addr: 127.0.1.10
        port: 7777
parameter:
max:
time:
