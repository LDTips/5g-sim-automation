db_uri: mongodb://localhost/open5gs
logger:
    file: /var/log/open5gs/{{ udr.log_filename | default ('udr.log') }}
sbi:
    server:
      no_tls: true
      cacert: /etc/open5gs/tls/ca.crt
      key: /etc/open5gs/tls/udr.key
      cert: /etc/open5gs/tls/udr.crt
    client:
      no_tls: true
      cacert: /etc/open5gs/tls/ca.crt
      key: /etc/open5gs/tls/udr.key
      cert: /etc/open5gs/tls/udr.crt
udr:
    sbi:
      - addr: 127.0.0.20
        port: 7777
scp:
    sbi:
      - addr: 127.0.1.10
        port: 7777
parameter:
max:
time:
