logger:
    file: /var/log/open5gs/{{ nrf.log_filename | default ('nrf.log') }}
sbi:
    server:
      no_tls: true
      cacert: /etc/open5gs/tls/ca.crt
      key: /etc/open5gs/tls/nrf.key
      cert: /etc/open5gs/tls/nrf.crt
    client:
      no_tls: true
      cacert: /etc/open5gs/tls/ca.crt
      key: /etc/open5gs/tls/nrf.key
      cert: /etc/open5gs/tls/nrf.crt
nrf:
    sbi:
      - addr:
        - 127.0.0.10
        - ::1
        port: 7777
scp:
    sbi:
      - addr: 127.0.1.10
        port: 7777
parameter:
max:
time:
